{
    "name": "zucchetti-pos/openboleto",
    "type": "library",
    "description": "Biblioteca para geração de boletos bancários.",
    "keywords": ["boleto", "boleto bancario"],
    "homepage": "http://github.com/zucchetti-pos/openboleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Garajau",
            "email": "kriansa@facebook.com"
        }
    ],
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8 || ^5.7.15"
    },
    "autoload": {
        "psr-4": {
            "OpenBoleto\\": "src/OpenBoleto/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenBoletoTest\\": "test/OpenBoletoTest/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    }
}
